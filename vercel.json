{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/static-build"
      }
    ],

    "headers": [
      {
        "source": "/assets/(.*)",
        "headers": [
          {
            "key": "Content-Type",
            "value": "audio/mpeg"
          },
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      },
      {
        "source": "/assets/music/(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      }
    ]

  }
