{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/assets/(.*)",
        "dest": "src/assets/$1",
        "headers": {
          "Content-Type": "audio/mpeg",

          "Cache-Control": "public, max-age=31536000, immutable"
        }
      }
    ],
    "headers": [
      {
        "source": "/assets/music/(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      }
    ]
  }
